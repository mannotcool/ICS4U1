package 3-6;

public class  {
    
}
public class SelectionSortTest
{
   public static void main( String[] args ) {
      // create object to perform selection sort
      SelectionSort sortArray = new SelectionSort( 10, SelectionSort.RANDOM );
      
      System.out.println( "UNSORTED ARRAY: " + sortArray );
      sortArray.bubbleSortv3();
      System.out.println( "\nSORTED ARRAY: "  + sortArray);
   } 
} 